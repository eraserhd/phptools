options:	()
description:	"C-style, new-style while statements."
result:
(compound-statement
  (while "condition" "statement")
  (while "condition" "statement"))

code:
<?php 
  while ("condition") {
    "statement";
  }

  while ("condition"):
    "statement";
  endwhile;
?>

